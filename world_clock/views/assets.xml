<odoo>
    <template id="assets_backend_world_clock" name="world_clock_assets" inherit_id="web.assets_backend">
        <xpath expr="//script[last()]" position="after">
            <script type="text/javascript" src="/world_clock/static/src/js/timer.js"></script>
        </xpath>
    </template>
     <template id="assets_backend_world_clock_css" inherit_id="web.assets_backend"
              name="World Clock Time">
        <xpath expr="." position="inside">
            <link rel="stylesheet" type="text/css" href="/world_clock/static/src/css/kanban_size.css"/>
        </xpath>
    </template>
     <template id="assets_frontend" inherit_id="website.assets_frontend">
        <xpath expr=".">

            <script type="text/javascript"
                    src="/world_clock/static/src/js/world_clock_render.js"/>
        </xpath>
    </template>
     <template id="assets_frontend_world_clock_css" inherit_id="web.assets_frontend"
              name="World Clock Time">
        <xpath expr="." position="inside">
            <link rel="stylesheet" type="text/css" href="/world_clock/static/src/css/world_clock.css"/>
        </xpath>
    </template>
</odoo>
